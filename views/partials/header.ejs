<header>
  <div class="logo">
    <a href="/">Members Only</a>
  </div>
  <h1>Welcome <%= currentUser  ? currentUser.username : "Guest" %>!</h1>
  <nav>
    <ul>
      <% if (currentUser) { %>
      <!-- Show sign-out button if user is logged in -->
      <li><a href="/log-out">Sign Out</a></li>
      <% } else { %>
      <!-- Show login button if user is not logged in -->
      <li><a href="/log-in">Login</a></li>
      <% } %>
    </ul>
  </nav>
</header>
